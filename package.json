{
  "name": "typescript-hapi-models-poc",
  "version": "0.10.0",
  "description": "POC project for TypeScript FHIR data models inspired by HAPI FHIR",
  "keywords": [
    "node",
    "typescript",
    "HAPI",
    "FHIR",
    "models",
    "POC"
  ],
  "author": "Joe Paquette <paqrat76@gmail.com>",
  "homepage": "https://github.com/Paqrat76/typescript-hapi-models-poc/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/Paqrat76/typescript-hapi-models-poc/issues",
    "email": "paqrat76@gmail.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo dist coverage docs",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:docs": "markdownlint './**/*.md' -i '**/node_modules/**' -i '**/CHANGELOG.md'",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "jest",
    "coverage": "jest --coverage",
    "jest:clear": "jest --clearCache",
    "typedoc": "typedoc src/index.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "zod": "^3.23.8"
  },
  "overrides": {
    "micromatch": "4.0.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "lint-staged": "^15.2.10",
    "markdownlint-cli": "^0.42.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typedoc-plugin-zod": "^1.2.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "engines": {
    "node": ">=20.9",
    "npm": ">=10.1"
  },
  "lint-staged": {
    "*.{json,yaml}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write",
      "npm run lint:docs"
    ],
    "*.{ts,js}": [
      "prettier --write",
      "eslint --max-warnings 0 --no-warn-ignored"
    ]
  },
  "repository": "github:Paqrat76/typescript-hapi-models-poc"
}
